{
    "builds":[{
        "src": "main_p/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize": "15mb", "runtime": "python3"}
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main_p/wsgi.py"
        }
    ],
    "installCommand": "pip install -r requirements.txt"
}